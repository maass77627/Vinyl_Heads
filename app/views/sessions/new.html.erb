user login

<h2> Log in </h2>
<%= form_tag("/sessions") do %>
    <p>
    <%= label_tag "username:" %>
    <%= text_field_tag "username" %>
    </p>
    <%= label_tag "email:" %>
    <%= text_field_tag "email" %>
    <p>
    <%= label_tag "password:" %>
    <%= text_field_tag "password" %>
    </p>
    <%= submit_tag "Login" %>
    <% end %>