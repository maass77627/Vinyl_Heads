<center>
user login
<%= form_for :user, url: login_path, method: :post do |f| %>
    <%= f.label :username %>
    <br>
    <%= f.text_field :username %>
    <br>
    <br>
    <%= f.label :password %>
    <br>
    <%= f.password_field :password %>
    <br>
    <%= f.label :email %>
    <br>
    <%= f.text_field :email %>
    <%= f.submit "Login" %>
    <% end %>
    </center>
