<h2> Sign up </h2>
<%= form_tag("/users") do %>
    <p>
    <%= label_tag "username:" %>
    <%= text_field_tag "email" %>
    </p>
    <%= label_tag "email:" %>
    <%= text_field_tag "email" %>
    <%= label_tag "password:" %>
    <%= text_field_tag "password" %>
    <%= submit_tag "Sign up" %>
    <% end %>